{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$assets/*": [
        "src/assets/*"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$store/*": [
        "src/store/*"
      ],
      "$/*": [
        "src/*"
      ],
      "$views/*": [
        "src/views/*"
      ],
      "$__test__/*": [
        "src/__test__/*"
      ],
      "$Sections/*": [
        "src/views/Sections/*"
      ],
      "$sections/*": [
        "src/views/sections/*"
      ],
      "$questions/*": [
        "src/views/sections/questions/*"
      ]
    }
  }
}